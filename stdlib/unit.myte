module std.unit

import std.bool.Bool
import std.hash.Hashable
import std.int.Int
import std.ops.Equatable
import std.string.{String, ToString}

builtin type Unit

methods Unit {
  implements Equatable

  override fun equals(other: Unit): Bool = true
}

methods Unit {
  implements Hashable

  override fun hash(): Int = 0
}

methods Unit {
  implements ToString

  override fun toString(): String = "()"
}