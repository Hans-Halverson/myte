module std.io

import std.io.file.File
import std.string.String

val STDIN: File = File (0)
val STDOUT: File = File (1)
val STDERR: File = File (2)

fun print(string: String) {
  File.__builtin_write(STDOUT[0], string.data, 0, string.size);
}

fun println(string: String) {
  print(string);
  print("\n");
}

fun eprint(string: String) {
  File.__builtin_write(STDERR[0], string.data, 0, string.size);
}

fun eprintln(string: String) {
  eprint(string);
  eprint("\n");
}
