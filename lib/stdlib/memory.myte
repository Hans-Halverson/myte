module std.memory

import std.bool.Bool
import std.int.Int

@(Builtin)
pub type Array<T>

methods Array<T> {
  @(Builtin)
  pub static fun new<T>(count: Int): Array<T>

  @(Builtin)
  pub static fun copy<T>(dest: Array<T>, destIndex: Int, src: Array<T>, srcIndex: Int, count: Int)

  @(Builtin)
  pub static fun isNull<T>(array: Array<T>): Bool
}