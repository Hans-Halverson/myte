module std.memory

import std.bool.Bool
import std.int.Int

pub builtin type Array<T>

methods Array<T> {
  builtin static fun new<T>(count: Int): Array<T>

  builtin static fun copy<T>(
    dest: Array<T>,
    destIndex: Int,
    src: Array<T>,
    srcIndex: Int,
    count: Int,
  )

  builtin static fun isNull<T>(array: Array<T>): Bool
}