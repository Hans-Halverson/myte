module test

fun testEq(x: int): bool = x == 3
fun testNe(x: int): bool = x != 3
fun testLt(x: int): bool = x < 3
fun testLe(x: int): bool = x <= 3
fun testGt(x: int): bool = x > 3
fun testGe(x: int): bool = x >= 3

fun testComparisonConditionals(x: int): int {
  if (x == 3) {
    return 1;
  } else if (x != 3) {
    return 2;
  } else if (x < 3) {
    return 3;
  } else if (x <= 3) {
    return 4;
  } else if (x > 3) {
    return 5;
  } else if (x >= 3) {
    return 6;
  } else {
    return 7;
  }
}

