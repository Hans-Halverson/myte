module test

import std.io.println

fun testToString() {
  println((0: Byte).toString());
  println((1: Byte).toString());
  println((12: Byte).toString());
  println((127: Byte).toString());
  println((-1: Byte).toString());
  println((-127: Byte).toString());
  println((-128: Byte).toString());
}

fun main() {
  testToString();
}