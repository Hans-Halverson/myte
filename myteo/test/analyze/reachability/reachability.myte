fun reachableAfterIf(): int {
  if (true) {
    return 1;
  }

  if (true) {
    2;
  } else {
    return 3;
  }

  4;
  return 5;
}

fun reachableAfterWhile(): int {
  while (true) {
    return 1;
  }

  2;
  return 3;
}

fun reachableAfterMatch(): int {
  val _ = 1;

  match (1) {
    | _ -> ()
  }

  match (1) {
    | _ -> return 1
    | _ -> ()
    | _ -> return 2
  }

  return 3;
