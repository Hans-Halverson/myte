module test

fun use<T>(x: T): T = x

fun orByteRegs(x: Byte, y: Byte): Byte = x | y
fun orByteImmReg(x: Byte): Byte = 60 | x
fun orByteRegImm(x: Byte): Byte = x | 60

fun orIntRegs(x: Int, y: Int): Int = x | y
fun orIntRegImm(x: Int): Int = x | 257
fun orIntImmReg(x: Int): Int = 257 | x

fun orLongRegs(x: Long, y: Long): Long = x | y
// First have to load long to register
fun orLongRegImm(x: Long): Long = x | 2147483648
fun orLongImmReg(x: Long): Long = 2147483648 | x

fun main() {
  use(orByteRegs);
  use(orByteImmReg);
  use(orByteRegImm);
  use(orIntRegs);
  use(orIntRegImm);
  use(orIntImmReg);
  use(orLongRegs);
  use(orLongRegImm);
  use(orLongImmReg);
}