module test

fun testBytes(x: Byte): Int =
  match (x) {
    | 0 -> 1
    | -1 -> 2
    | 5 -> 3
    | _ -> 4
  }

fun testInts(x: Int): Int =
  match (x) {
    | 0 -> 1
    | -1 -> 2
    | 5 -> 3
    | _ -> 4
  }

fun testLongs(x: Long): Int =
  match (x) {
    | 0 -> 1
    | -1 -> 2
    | 5 -> 3
    | _ -> 4
  }

fun testCharLiterals(x: Byte): Int =
  match (x) {
    | 'a' -> 1
    | 'b' -> 2
    | '0' -> 3
    | _ -> 4
  }

fun use<T>(x: T) {}

fun main() {
  use(testBytes);
  use(testInts);
  use(testLongs);
  use(testCharLiterals);
}