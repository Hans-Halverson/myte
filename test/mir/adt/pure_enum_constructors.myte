// Pure enum that fits into Byte
type ByteEnum = 
  | Variant1
  | Variant2
  | Variant3
  | Variant4

fun useByteEnum(x: ByteEnum) {}

fun testPureEnum1Values(): (ByteEnum, ByteEnum) {
  useByteEnum(Variant1);
  useByteEnum(Variant2);
  val x = (Variant4, Variant3);
  return x;
}

