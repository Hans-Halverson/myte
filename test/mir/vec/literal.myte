module test

fun emptyVec() {
  val x: Vec<Int> = [];
  val y: Vec<Bool> = [];
}

fun oneElementVec() {
  [1];
  [(2: Byte)];
}

fun multiElementVec() {
  [5, 6, 7];
  [false, true, false, true];
}

type ZeroSize (Unit)

fun zeroSizeElementsVec() {
  [(), id(()), ()];
  [ZeroSize (()), id(ZeroSize (())), ZeroSize (())];
}

fun id<T>(x: T): T = x

fun use<T>(x: T) {}

fun main() {
  use(emptyVec);
  use(oneElementVec);
  use(multiElementVec);
  use(zeroSizeElementsVec);
}