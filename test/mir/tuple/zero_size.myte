module test

fun testZeroSizeTuple() {
  val x = (getUnit(), getUnit());
  use(x);

  use(x[0]);
  use(x[1]);
}

fun testTupleWithZeroSizeFields() {
  val x = (getUnit(), 1, (), id(()), true);
  use(x);

  use(x[0]);
  use(x[1]);
  use(x[2]);
  use(x[3]);
  use(x[4]);
}

fun getUnit() = ()

fun id<T>(x: T): T = x

fun use<T>(x: T) {}

fun main() {
  use(testZeroSizeTuple);
  use(testTupleWithZeroSizeFields);
}