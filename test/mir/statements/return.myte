module test

fun test1(): Unit {
  use(1);
  return;
}

fun test2(): Int {
  return 99;
}

fun returnPreventsContinue() {
  // Continue should not follow ret
  if (true) {
    return;
  }

  while (true) {
    return;
  }
}

fun use<T>(x: T) {}

fun main() {
  use(test1);
  use(test2);
  use(returnPreventsContinue);
}