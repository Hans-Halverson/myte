module test

fun testWithoutAltern() {
  use(1);
  if (true) {
    use(2);
  }

  use(3);
}

fun testWithAltern() {
  use(1);
  if (true) {
    use(2);
  } else {
    use(3);
  }

  use(4);
}

fun nested() {
  use(1);
  if (true) {
    use(2);

    if (false) {
      use(3);
    } else {
      use(4);
    }

    use(5);
  } else {
    use(6);

    if (false) {
      use(7);
    } else {
      use(8);
    }

    use(9);
  }
}

fun use<T>(x: T) {}

fun main() {
  use(testWithoutAltern);
  use(testWithAltern);
  use(nested);
}