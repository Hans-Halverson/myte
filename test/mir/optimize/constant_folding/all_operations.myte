module test

fun use<T>(x: T): T = x

fun unit(): Unit = ()

fun testInt() {
  use(-(56));

  use(1 + 2);
  use(1 - 2);
  use(2 * 7);
  use(24 / 3);
  use(19 % 3);
  use(0b1100 & 0b1010);
  use(0b1100 | 0b1010);
  use(0b1100 ^ 0b1010);
  use(1 << 4);
  use(-1 >> 2);
  use(-1 >>> 2);

  use((5: Int) > 7);
}

fun testByte() {
  use(-(56));

  use(1 + 2);
  use(1 - 2);
  use(2 * 7);
  use(24 / 3);
  use(19 % 3);
  use(0b1100 & 0b1010);
  use(0b1100 | 0b1010);
  use(0b1100 ^ 0b1010);
