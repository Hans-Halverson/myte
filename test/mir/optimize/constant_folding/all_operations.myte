fun testBool() {
  use(!true);

  use(true && false);
  use(true || false);
  use(true == false);
  use(true != false);
}

fun testUnit() {
  use(() == ());
  use(() != ());
  use(unit() == unit());
  use(unit() != unit());
}

// Shifts only use lowest byte of right hand side
fun testShift() {
  use(1 << 258);
  use(1 << 258);
}

fun main() {}