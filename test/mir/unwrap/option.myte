module test

fun testUnwrapOption(x: Option<Long>): Option<Long> {
  use(x?);
  return Some (1);
}

fun testUnwrapOptionDifferentType(x: Option<String>): Option<Long> {
  use(x?);
  return Some (1);
}

fun testDoubleUnwrap(x: Option<Option<Long>>): Option<Long> {
  use(x??);
  return Some (1);
}

type ZeroSize (Unit)

fun testZeroSizeUnwrap(x: Option<ZeroSize>): Option<Long> {
  use(x?);
  return Some (1);
}

fun use<T>(x: T) {}

fun main() {
  use(testUnwrapOption);
  use(testUnwrapOptionDifferentType);
  use(testDoubleUnwrap);
  use(testZeroSizeUnwrap);
}